<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.bowen.architecture.architecture1.storeMgr.dao.StoreDAO">
     <insert id="create" parameterType="StoreM">
     	insert into tb1_store (goodsUuid,storeNum) 
     	 values(#{goodsUuid},#{storeNum})
     </insert>
     <update id="update" parameterType="StoreM">
     	update tb1_store set goodsUuid=#{goodsUuid},storeNum=#{storeNum}
     	   	where uuid=#{uuid}
     </update>
     <delete id="delete" parameterType="int">
     	delete from tb1_store where uuid=#{uuid}
     </delete>
     
    <select id="getByUuid" parameterType="int" resultType="StoreM">  
        select * from tb1_store where uuid=#{uuid}
    </select>  
    <select id="getByConditionPage" parameterType="StoreQM" resultType="StoreM">  
        select * from tb1_store
        <where>
        	<if test="uuid!=null &amp;&amp; uuid > 0">
        		and uuid=#{uuid}
        	</if>  
        	
        </where>
    </select>

    <select id="getByGoodsUuid" parameterType="StoreQM" resultType="StoreM">
        select * from tb1_store where goodsUuid=#{goodsUuid}
    </select>
</mapper>